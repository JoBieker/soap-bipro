            xmlns:allg="http://www.bipro.net/namespace/allgemein"
            xmlns:bipro="http://www.bipro.net/namespace"
            xmlns:partner="http://www.bipro.net/namespace/partner"
            xmlns:tarif="http://www.bipro.net/namespace/versicherung/tarifierung"
            targetNamespace="http://www.bipro.net/namespace/fahrzeug"
            bipro:Name="bipro-fahrzeug-2.6.0.xsd"
            bipro:Version="2.6.0"
            elementFormDefault="qualified"
            attributeFormDefault="qualified">
   <xsd:annotation>
      <xsd:appinfo>BiPRO Release 2.6
		(Build: 20160617-1214)</xsd:appinfo>
   </xsd:annotation>
   <xsd:import namespace="http://www.bipro.net/namespace/datentypen"
               schemaLocation="bipro-datentypen-2.6.0.xsd"/>
   <xsd:import namespace="http://www.bipro.net/namespace/partner"
               schemaLocation="bipro-partner-2.6.0.xsd"/>
   <xsd:import namespace="http://www.bipro.net/namespace/allgemein"
               schemaLocation="bipro-allgemein-2.6.0.xsd"/>
   <xsd:import namespace="http://www.bipro.net/namespace/basis"
               schemaLocation="bipro-basis-2.6.0.xsd"/>
   <xsd:complexType name="CT_LKWNutzung">
      <xsd:complexContent>
         <xsd:extension base="CT_Fahrzeugnutzung">
            <xsd:sequence>
               <xsd:element name="Gefahrgut" type="dt:ST_Gefahrgut" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Fahrgebiet" type="dt:ST_Fahrgebiet" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Fahrgebiet oder Nutzungsgebiet</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="TransportierteGueter" type="dt:ST_TransportierteGueter" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Gibt an ob eigene oder fremde Güter transportiert werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Bus">
      <xsd:complexContent>
         <xsd:extension base="CT_Fahrzeug">
            <xsd:sequence>
               <xsd:element name="AnzahlPlaetze" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Gesamtzahl der Sitz- und Stehplätze</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BusNutzung" type="CT_BusNutzung" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_VersicherungsbestaetigungKfz">
      <xsd:complexContent>
         <xsd:extension base="basis:CT_Objekt">
            <xsd:sequence>
               <xsd:element name="Versicherungsbestaetigungsnummer" type="dt:ST_VbNummer" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="VbArt" type="dt:ST_VbArt" minOccurs="0" maxOccurs="1" nillable="true"/>
               <xsd:element name="VbAbrufbarAb" type="dt:ST_Zeitstempel" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="VbAbrufbarBis" type="dt:ST_Datum" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Zulassungsfahrten" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="MietfahrzeugFuerSelbstfahrer" type="dt:ST_JaNein" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="DatumVersicherungsbeginn" type="dt:ST_Datum" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="HalterAbweichendVonVersicherungsnehmer" type="dt:ST_JaNein" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="VbAusgegebenDurch" type="dt:ST_VbAusgegebenDurch" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">VB ausgegeben durch Vermittler oder Versicherer</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Ablaufdatum" type="dt:ST_Datum" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Ablaufdatum der Versicherungsbestätigung für eVB insbesondere im Kontingent</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="KFZFuerBehinderteSchuelerKindergartentraeger" type="dt:ST_JaNein"
                            minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Dieses KFZ wird für Behinderte, Schüler oder Kindergartenträger verwendet.
Nach §6 FZV (Fahrzeugzulassungsverordnung) ist der Verwendungszweck eines Farzeuges bei der Erstellung einer eVB anzugeben.
Wird dieses Attribut nicht geliefert, MUSS es als "Nein" interpretiert werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Krankenwagen" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Dieses KFZ wird als Krankenwagen verwendet.
Nach §6 FZV (Fahrzeugzulassungsverordnung) ist der Verwendungszweck eines Farzeuges bei der Erstellung einer eVB anzugeben.
Wird dieses Attribut nicht geliefert, MUSS es als "Nein" interpretiert werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Mietfahrzeug" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Dieses KFZ wird als Mietfahrzeug verwendet. Mietwagen sind im gesetzlichen Sinne Fahrzeuge, deren Einsatz gemietet wird einschließlich der Fahrer-Dienste.
Nach §6 FZV (Fahrzeugzulassungsverordnung) ist der Verwendungszweck eines Fahrzeuges bei der Erstellung einer eVB anzugeben.
Wird dieses Attribut nicht geliefert, MUSS es als "Nein" interpretiert werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Linienbus" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Dieses KFZ wird als Linienbus verwendet.
Nach §6 FZV (Fahrzeugzulassungsverordnung) ist der Verwendungszweck eines Farzeuges bei der Erstellung einer eVB anzugeben.
Wird dieses Attribut nicht geliefert, MUSS es als "Nein" interpretiert werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Taxi" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1" nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Dieses KFZ wird als Taxi verwendet.
Nach §6 FZV (Fahrzeugzulassungsverordnung) ist der Verwendungszweck eines Fahrzeuges bei der Erstellung einer eVB anzugeben.
Wird dieses Attribut nicht geliefert, MUSS es als "Nein" interpretiert werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="SonstigeBesondereVerwendung" type="dt:ST_JaNein" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Dieses KFZ wird zur sonstiger besonderen Verwendung genutzt.
Nach §6 FZV (Fahrzeugzulassungsverordnung) ist der Verwendungszweck eines Farzeuges bei der Erstellung einer eVB anzugeben.
Wird dieses Attribut nicht geliefert, MUSS es als "Nein" interpretiert werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="OhneBesondereVerwendung" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Dieses KFZ wird ohne besonderen Verwendung genutzt.
Nach §6 FZV (Fahrzeugzulassungsverordnung) ist der Verwendungszweck eines Farzeuges bei der Erstellung einer eVB anzugeben.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_PKWNutzung">
      <xsd:complexContent>
         <xsd:extension base="CT_FahrzeugnutzungErweitert">
            <xsd:sequence>
               <xsd:element name="BegleitetesFahrenVN" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="BegleitetesFahrenNutzer" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_AnhaengerNutzung">
      <xsd:complexContent>
         <xsd:extension base="CT_Fahrzeugnutzung">
            <xsd:sequence>
               <xsd:element name="Gefahrgut" type="dt:ST_Gefahrgut" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Leasinggeber">
      <xsd:complexContent>
         <xsd:extension base="partner:CT_Partnerrolle">
            <xsd:sequence/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Kennzeichenart">
      <xsd:complexContent>
         <xsd:extension base="basis:CT_Objekt">
            <xsd:sequence>
               <xsd:element name="MerkmalKennzeichenart" type="dt:ST_Kennzeichenart" minOccurs="1"
                            maxOccurs="1"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Sonderausstattung">
      <xsd:complexContent>
         <xsd:extension base="basis:CT_Objekt">
            <xsd:sequence>
               <xsd:element name="ArtID" type="dt:STE_Sonderausstattung" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Beschreibung" type="dt:ST_Text050" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Betrag" type="dt:ST_Betrag" minOccurs="0" maxOccurs="1" nillable="true"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Fahrer">
      <xsd:complexContent>
         <xsd:extension base="partner:CT_Partnerrolle">
            <xsd:sequence>
               <xsd:element name="UeberwiegenderFahrer" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Dieser Fahrer ist der überwiegende Nutzer des Fahrzeugs</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BerechtigterNutzer" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">JA = Erlaubnis des Fahrzeughalters zur Nutzung des Fahrzeugs vorhanden.
NEIN = Erlaubnis des Fahrzeughalters zur Nutzung des Fahrzeugs nicht vorhanden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BegleiteterFahrer" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Zur eindeutigen Zuordnung begleiteter Fahrer.
Ja = Fahrer ist begleiteter Fahrer.
Nein = sonst</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Begleitperson" type="CT_Begleitperson" minOccurs="0"
                            maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Betriebsleistung">
      <xsd:annotation>
         <xsd:documentation xml:lang="de">Messgröße für die Nutzung eines technischen Geräts, z. B. eines Fahrzeugs. Die Betriebsleistung kann z. B. die Fahrleistung (Kilometerstand) oder die Nutzungsdauer (Betriebsstunden) sein.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="basis:CT_Objekt">
            <xsd:sequence>
               <xsd:element name="Wert" type="dt:ST_Dezimalzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Wert der Betriebsleistung gemäß Werteinheit, z.B. Kilometerstand, Betriebsstunden.
Fehlt die Werteinheit, MUSS der Wert in Kilometer angegeben werden (also der Kilometerstand).</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Werteinheit" type="dt:ST_Werteinheit" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Werteinheit der Betriebsleistung, in Kilometer, Meilen oder Stunden.
Fehlt die Werteinheit, MUSS die Betriebsleistung in Kilometer angegeben werden (also der Kilometerstand).</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Ermittlungsdatum" type="dt:ST_Datum" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Datum der Ermittlung der Betriebsleistung.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Ermittlungsart" type="dt:ST_Ermittlungsart" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Art der Ermittlung der Betriebsleistung.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_LKWLeicht">
      <xsd:annotation>
         <xsd:documentation xml:lang="de">WKZ 2xx</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="CT_Fahrzeug">
            <xsd:sequence>
               <xsd:element name="ZulaessigesGesamtgewicht" type="dt:ST_Dezimalzahl" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Zulässiges Gesamtgewicht in kg.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Aufbauten" type="dt:STE_Aufbauten" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="AufbautenText" type="dt:ST_Text030" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Beschreibung der Aufbauten als Text, falls im Attribut Aufbauten der Wert "99" geschlüsselt ist.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnhaengerBetrieb" type="dt:ST_JaNeinKeineAntwort" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="LKWNutzung" type="CT_LKWNutzung" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_CampingKfzNutzung">
      <xsd:complexContent>
         <xsd:extension base="CT_FahrzeugnutzungErweitert">
            <xsd:sequence/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_CampingKfz">
      <xsd:annotation>
         <xsd:documentation xml:lang="de">WKZ 127</xsd:documentation>
      </xsd:annotation>
