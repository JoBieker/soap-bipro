               <xsd:element name="ArtDerZulassungBeimVorbesitzer" type="dt:ST_ArtDerZulassung"
                            minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Art der Zulassung beim direkten Vorbesitzer des aktuellen Besitzers.</xsd:documentation>
                  </xsd:annotation>
