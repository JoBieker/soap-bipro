   <xsd:complexType name="CT_FahrzeugnutzungErweitert" abstract="true">
      <xsd:complexContent>
         <xsd:extension base="CT_Fahrzeugnutzung">
            <xsd:sequence>
               <xsd:element name="Nutzungsart" type="dt:ST_Nutzungsart" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Gemäß GDV-Schlüssel Satzart 0210.050/2 Feld 22</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Jahresfahrleistung" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">in km</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="JahresfahrleistungUnbegrenzt" type="dt:ST_JaNein" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Nutzerkreis" type="dt:ST_Nutzerkreis" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="AlterJuengsterMaennlicherNutzer" type="dt:ST_Ganzzahl" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Bei Unisex-Tarifen DARF dieses Attribut NICHT mehr benutzt werden. Statt dessen MUSS das Attribut AlterJuengsterNutzer verwendt werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GeburtsjahrJuengsterMaennlicherNutzer" type="dt:ST_Jahr" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Bei Unisex-Tarifen DARF dieses Attribut NICHT mehr benutzt werden. Statt dessen MUSS das Attribut GeburtsjahrJuengsterNutzer verwendt werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AlterJuengsterWeiblicherNutzer" type="dt:ST_Ganzzahl" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Bei Unisex-Tarifen DARF dieses Attribut NICHT mehr benutzt werden. Statt dessen MUSS das Attribut AlterJuengsterNutzer verwendt werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GeburtsjahrJuengsterWeiblicherNutzer" type="dt:ST_Jahr" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Bei Unisex-Tarifen DARF dieses Attribut NICHT mehr benutzt werden. Statt dessen MUSS das Attribut GeburtsjahrJuengsterNutzer verwendt werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AlterJuengsterNutzerUnbekannt" type="dt:ST_JaNein" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="AlterAeltesterNutzer" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="GeburtsjahrAeltesterNutzer" type="dt:ST_Jahr" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Geburtsjahr des ältesten Fahrzeugnutzers, unabhängig vom Geschlecht.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AlterAeltesterNutzerUnbekannt" type="dt:ST_JaNein" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="AlterJuengsterNutzer" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Alter des jüngsten Fahrzeugnutzers, unabhängig vom Geschlecht.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GeburtsjahrJuengsterNutzer" type="dt:ST_Jahr" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Geburtsjahr des jüngsten Fahrzeugnutzers, unabhängig vom Geschlecht.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AlleFahrerHabenEUFuehrerschein" type="dt:ST_JaNein" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Angabe, ob alle Fahrer einen in einem EU-Land ausgestellten Führerschein besitzen.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GeburtsdatumJuengsterFahrer" type="dt:ST_Datum" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Geburtsdatum des jüngsten Fahrers.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_SFRBerechtigter">
      <xsd:complexContent>
         <xsd:extension base="partner:CT_Partnerrolle">
            <xsd:sequence/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_BusNutzung">
      <xsd:complexContent>
         <xsd:extension base="CT_Fahrzeugnutzung">
            <xsd:sequence>
               <xsd:element name="Fahrgebiet" type="dt:ST_Fahrgebiet" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Fahrgebiet oder Nutzungsgebiet</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Verwendung" type="dt:ST_Text050" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Reifen">
      <xsd:annotation>
         <xsd:documentation xml:lang="de">Der äußere Teil eines Rades, auf dem es abrollt.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="basis:CT_Objekt">
            <xsd:sequence>
               <xsd:element name="Achse" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1" nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Achse, auf der das Rad montiert ist. Die Achsen werden von vorne nach hinten durchnummeriert. Bei einem PKW also:
Achse 1 = Vorderachse
Achse 2 = Hinterachse</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Position" type="dt:ST_Radposition" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Position des Rades auf der Achse bzw. als Reserverad.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Profiltiefe" type="dt:ST_Dezimalzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Profiltiefe des Reifens in Millimeter (mm).</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Kennzeichen">
      <xsd:complexContent>
         <xsd:extension base="basis:CT_Objekt">
            <xsd:sequence>
               <xsd:element name="AmtlichesKennzeichen" type="dt:ST_AmtlichesKennzeichen" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="DatumKennzeichenGueltigBis" type="dt:ST_Datum" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="AblauftageKurzzeitkennzeichen" type="dt:ST_Ganzzahl" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="BetriebszeitraumVon" type="dt:ST_Monat" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Für Saison-Kennzeichen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BetriebszeitraumBis" type="dt:ST_Monat" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Für Saison-Kennzeichen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Zulassungsbezirksnummer" type="dt:ST_Zulassungsbezirksnummer"
                            minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="RegioPostleitzahl" type="dt:ST_Postleitzahl" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="RegioOrt" type="dt:ST_Text030" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Die Postleitzahl alleine ist aber nicht immer eindeutig. Als Beispiel dient hier die PLZ '21039', zu der es die Orte Börnsen, Fahrendorf, Hamburg und Escheburg gibt. Zur korrekten Regionalklassen-Ermittlung sind daher die Attribute RegioPostleitzahl und RegioOrt unerlässlich.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Kennzeichenart" type="CT_Kennzeichenart" minOccurs="0"
                            maxOccurs="unbounded"/>
               <xsd:element name="Land" type="dt:ST_Laenderkennzeichen" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Land, in dem das Kennzeichen registriert ist.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="GruenesKennzeichen" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">JA = Kennzeichen ist ein grünes Kennzeichen
NEIN = Kennzeichen ist kein grünes Kennzeichen
Wird das Attribut nicht geliefert, MUSS es als mit 'NEIN' belegt interpretiert werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Wechselkennzeichen" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Bei bereits zugelassenen Fahrzeugen enthält dieses Attribut die Information, ob das Kennzeichen ein Wechselkennzeichen ist:
JA = Das Kennzeichen ist ein Wechselkennzeichen
NEIN = Das Kennzeichen ist kein Wechselkennzeichen
Beim eVB-Prozess hat das Attribut folgende Semantik:
JA = Die eVB gilt nur für ein Wechselkennzeichen
NEIN = Die Nutzung der eVB für Wechselkennzeichen ist ausgeschlossen
Fehlt das Attribut = Die eVB hat keine Einschränkung bzgl. Wechselkennzeichen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Fahrzeug">
      <xsd:complexContent>
         <xsd:extension base="allg:CT_Gegenstand">
            <xsd:sequence>
               <xsd:element name="Eigentumsverhaeltnis" type="dt:ST_Eigentumsverhaeltnis" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Finanzierungsart gemäß GDV-Schlüssel Satzart 0210.050/2 Feld 23</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Erstzulassung" type="dt:ST_Datum" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="FahrzeugGesamtNeuwert" type="allg:CT_Neuwert" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="FahrzeugGesamtZeitwert" type="allg:CT_Zeitwert" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Fahrzeugidentifizierungsnummer" type="dt:ST_Text025" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Flottenkennzeichen" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Fahrzeug ist Teil einer Flotte.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Herstellername" type="dt:ST_Text025" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Herstellerschluessel" type="dt:ST_Herstellerschluessel" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Hubraum" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Hubraum in ccm</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Modellname" type="dt:ST_Text050" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="MotorLeistung" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Motorleistung in KW. Wird bei Anhängern nicht verwendet.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Typschluessel" type="dt:ST_Typschluessel" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="VarianteVersionSchluessel" type="dt:ST_VarianteVersionSchluessel"
                            minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Wagniskennziffer" type="dt:ST_Wagniskennziffer" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="WagniskennzifferText" type="dt:ST_Text050" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Fahrzeugart" type="dt:ST_Fahrzeugart" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Über die Fahrzeugart kann beim eVB-Abruf optional die Gültigkeit der eVB auf eine bestimmte Fahrzeugart (PKW, LKW, Anhänger etc.) eingeschränkt werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Kennzeichen" type="CT_Kennzeichen" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Sicherungsglaeubiger" type="partner:CT_Sicherungsglaeubiger"
                            minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Sonderausstattung" type="CT_Sonderausstattung" minOccurs="0"
                            maxOccurs="unbounded"/>
               <xsd:element name="VersicherungsbestaetigungKfz" type="CT_VersicherungsbestaetigungKfz"
                            minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Leasingnehmer" type="CT_Leasingnehmer" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Leasinggeber" type="CT_Leasinggeber" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="SFRBerechtigter" type="CT_SFRBerechtigter" minOccurs="0" maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Halter" type="CT_Halter" minOccurs="0" maxOccurs="1" nillable="true"/>
               <xsd:element name="Fahrer" type="CT_Fahrer" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="Wegfahrsperre" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Gibt an ob das Fahrzeug über eine Wegfahrsperre verfügt.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Staerke" type="dt:ST_Dezimalzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Stärke des Fahrzeugs gemäß Staerkeeinheit (je nach Wagniskennziffer in: KW, CCM, Tonnen, Plätze)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Staerkeeinheit" type="dt:ST_Werteinheit" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Einheit zur Staerke (KW, CCM, Tonnen, Plätze)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Finanzierungslaufzeit" type="allg:CT_Zeitraum" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Fahrerassistenzsystem" type="CT_Fahrerassistenzsystem" minOccurs="0"
                            maxOccurs="unbounded"/>
               <xsd:element name="FahrzeugGesamtKaufpreis" type="allg:CT_Kaufpreis" minOccurs="0"
                            maxOccurs="1"
                            nillable="true"/>
               <xsd:element name="Zulassungspflichtig" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Ja = zulassungspflichtig
Nein = nicht zulassungspflichtig
Ist das Attribut nicht belegt, MUSS es als mit "Ja" belegt interpretiert werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Baujahr" type="dt:ST_Jahr" minOccurs="0" maxOccurs="1" nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Baujahr des Fahrzeugs.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Reimport" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1" nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Typschlüssel des Vergleichsfahrzeugs wird im Typschluessel übermittelt.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnzahlDerVorbesitzer" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl der Vorbesitzer gemäß Fahrzeugbrief.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="NaechsteHauptuntersuchung" type="dt:ST_MonatJahr" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Datum der nächsten Hauptuntersuchung.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Dachart" type="dt:ST_Fahrzeugdachart" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Art des Dachs des Fahrzeugs</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Farbe" type="dt:ST_Text050" minOccurs="0" maxOccurs="1" nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Farbe des Fahrzeugs. Es kann u.a. die Farbe sein, die ein Schadenmelder in die Schadenmeldung eingibt (also eher grau als anthrazit-metallic) oder die werbliche Farbbezeichnung sein. </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Sitzbezug" type="dt:ST_Sitzbezug" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Art der Sitzbezüge.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Motorart" type="dt:ST_Motorart" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Art des Fahrzeugmotors.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Getriebeart" type="dt:ST_Getriebeart" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Art des Getriebes.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnzahlGaenge" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl der Gänge, ohne Rückwärtsgänge.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Felgenart" type="dt:ST_Felgenart" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Art der Felgen.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Reserveradart" type="dt:ST_Reserveradart" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Art des Reserverades.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnzahlAchsen" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl der Achsen.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnzahlAngetriebeneAchsen" type="dt:ST_Ganzzahl" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl der angetriebenen Achsen.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnzahlTueren" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl der Türen.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnzahlElektrischeFensterheber" type="dt:ST_Ganzzahl" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl der elektrischen Fensterheber.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnzahlSitzheizungen" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl der Sitzheizungen.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="AnzahlAirbags" type="dt:ST_Ganzzahl" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl der Airbags.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Anhaengerkupplung" type="dt:ST_JaNein" minOccurs="0" maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">JA = Das Fahrzeug hat eine Anhängerkupplung
NEIN = Das Fahrzeug hat keine Anhängerkupplung</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Frontscheibenreparaturen" type="dt:ST_Ganzzahl" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Anzahl Reparaturen an der aktuell eingebauten Frontscheibe.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ErsteZulassungAufDenVNOderHalter" type="dt:ST_Datum" minOccurs="0"
                            maxOccurs="1"
                            nillable="true">
                  <xsd:annotation>
                     <xsd:documentation xml:lang="de">Erste Zulassung dieses Fahrzeuges auf den aktuellen VN oder Halter.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Reifen" type="CT_Reifen" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element name="Betriebsleistung" type="CT_Betriebsleistung" minOccurs="0"
                            maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CT_Fahrerassistenzsystem">
      <xsd:annotation>
         <xsd:documentation xml:lang="de">Elektronische Zusatzeinrichtung in Kraftfahrzeugen zur Unterstützung des Fahrers in bestimmten Fahrsituationen.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
