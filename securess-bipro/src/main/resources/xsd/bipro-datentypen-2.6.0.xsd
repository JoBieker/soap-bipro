            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="04006">
            <xsd:annotation>
               <xsd:documentation xml:lang="de">Das in VerfuegbarBis angegebene Datum ist überschritten.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="99999">
