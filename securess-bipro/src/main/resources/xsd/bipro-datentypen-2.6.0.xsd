               <xsd:documentation xml:lang="de">WA (Warschauer Abkommen)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="21">
            <xsd:annotation>
               <xsd:documentation xml:lang="de">BGB (Bürgerliches Gesetzbuch)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="99">
            <xsd:annotation>
               <xsd:documentation xml:lang="de">Sonstige Haftungsgrundlage</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
