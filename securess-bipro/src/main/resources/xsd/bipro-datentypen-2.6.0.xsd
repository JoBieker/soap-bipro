               <xsd:documentation xml:lang="de">Sonstiges</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="05000">
            <xsd:annotation>
               <xsd:documentation xml:lang="de">Ergebnisermittlung erfolgreich</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="05001">
            <xsd:annotation>
               <xsd:documentation xml:lang="de">Ergebnisermittlung fehlgeschlagen</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="05100">
