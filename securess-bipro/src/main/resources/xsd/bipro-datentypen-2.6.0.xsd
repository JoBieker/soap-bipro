               <xsd:annotation>
                  <xsd:documentation>Aufgrund von Produkt-Bündelung gewährter Nachlass </xsd:documentation>
               </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Dauer">
               <xsd:annotation>
                  <xsd:documentation>Aufgrund der Vertragslaufzeit gewährter Nachlass</xsd:documentation>
               </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Schadenfreiheit">
               <xsd:annotation>
                  <xsd:documentation>Durch Schadensfreiheit gewährter Nachlass.Gilt nicht für KFZ, da der Schadenfreiheitsrabatt dort über die SF-Klasse definiert wird.</xsd:documentation>
               </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Sonderkondition">
