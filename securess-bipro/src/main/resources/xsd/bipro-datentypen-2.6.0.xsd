            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="05202">
            <xsd:annotation>
               <xsd:documentation xml:lang="de">Förderdatenantrag angenommen</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="05203">
